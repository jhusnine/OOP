class Player:
    def __init__(self, name, health, attack_power):
        self.name = name
        self.health = health
        self.attack_power = attack_power
        
    def attack(self, target):
        target.health = target.health - self.attack_power
        print(f"{self.name} attack {self.kuki} for {self.attack_power}")
        print(f"{target.name} Health: {target.health})
        
nilou = Player("Nilou", 2000, 100)
kuki = Player("Kuki", 2500, 150)

nilou.attack(kuki)

print(nilou.name, nilou.health, nilou.attack_power)
print(kuki.name, kuki.health, kuki.attack_power)
