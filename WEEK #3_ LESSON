__INIT__() - INITIALIZED

class character:
    def __init__(self, name, gun):
        self.name = name
        self.gun = gun
        
codm = character("Artery", 18)
codm2 = character("Ghost", 19)
print(codm.gun, codm2.gun)


METHODS

class character:
    def __init__(self, name, gun):
        self.name = name
        self.gun = gun
        
    def describe(self):
        print(f"Si {self.name} ay {self.gun} smg")
        
codm = character("Artery", "Shooter")
codm.describe()

class character:
    def __init__(self, name):
        self.name = name

    def describe(self,role):
        print(f"{self.name} is a {role} hero.")
        
ml1 = character("Layla")
ml1.describe("Marksman")'''
